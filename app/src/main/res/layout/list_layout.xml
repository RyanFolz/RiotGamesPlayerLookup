<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="fill_parent">

    <LinearLayout
        android:id="@+id/game_one_title_bar"
        android:layout_width="fill_parent"
        android:layout_height="12dp"
        android:layout_marginTop="4dp">

        <TextView
            android:id="@+id/what_type_of_game_game_one"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:textColor="#ffffffff"
            android:textSize="9dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/game_one"
        android:layout_width="fill_parent"
        android:layout_height="70dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/champion_played_game_one"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginLeft="3dp"
            android:layout_marginTop="3dp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="6dp"
                android:layout_marginTop="4dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/kills_game_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ff00b422"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/game_one_slash_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ff9c9c9c"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/deaths_game_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ffff0028"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/game_one_slash_two"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ff9c9c9c"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/assists_game_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ff0000ff"
                    android:textSize="16dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|center_horizontal"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="2dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/gold_game_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:textColor="#ffcd8c00"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/cs_game_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:textColor="#ffff00fe"
                    android:textSize="14dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp">

                <ImageView
                    android:id="@+id/game_one_summoner_spell_one"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_marginLeft="4dp" />

                <ImageView
                    android:id="@+id/game_one_summoner_spell_two"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_marginLeft="4dp" />

            </LinearLayout>
        </LinearLayout>

        <GridLayout
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:columnCount="2"
            android:rowCount="3">

            <ImageView
                android:id="@+id/game_one_item_one"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_column="0"
                android:layout_row="0" />

            <ImageView
                android:id="@+id/game_one_item_two"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_column="1"
                android:layout_marginLeft="3dp"
                android:layout_row="0" />

            <ImageView
                android:id="@+id/game_one_item_three"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_column="0"
                android:layout_marginTop="3dp"
                android:layout_row="1" />

            <ImageView
                android:id="@+id/game_one_item_four"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_column="1"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_row="1" />

            <ImageView
                android:id="@+id/game_one_item_five"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_column="0"
                android:layout_marginTop="3dp"
                android:layout_row="2" />

            <ImageView
                android:id="@+id/game_one_item_six"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_column="1"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_row="2" />


        </GridLayout>

        <GridLayout
            android:layout_width="10dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:columnCount="1"
            android:rowCount="5">

            <ImageView
                android:id="@+id/game_one_team_one_player_one"
                android:layout_width="10dp"

                android:layout_height="10dp"
                android:layout_column="0"
                android:layout_marginTop="3dp"
                android:layout_row="0" />

            <ImageView
                android:id="@+id/game_one_team_one_player_two"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_column="0"

                android:layout_marginTop="3dp"
                android:layout_row="1" />

            <ImageView
                android:id="@+id/game_one_team_one_player_three"
                android:layout_width="10dp"

                android:layout_height="10dp"
                android:layout_column="0"
                android:layout_marginTop="3dp"
                android:layout_row="2" />

            <ImageView

                android:id="@+id/game_one_team_one_player_four"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_column="0"
                android:layout_marginTop="3dp"
                android:layout_row="3" />

            <ImageView

                android:id="@+id/game_one_team_one_player_five"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_column="0"
                android:layout_marginTop="3dp"
                android:layout_row="4" />


        </GridLayout>

        <LinearLayout
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="4dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/game_one_team_one_player_one_name"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:lines="1"
                android:textSize="8dp" />

            <TextView
                android:id="@+id/game_one_team_one_player_two_name"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:lines="1"
                android:textSize="8dp" />

            <TextView
                android:id="@+id/game_one_team_one_player_three_name"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:lines="1"
                android:textSize="8dp" />

            <TextView
                android:id="@+id/game_one_team_one_player_four_name"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:lines="1"
                android:textSize="8dp" />

            <TextView
                android:id="@+id/game_one_team_one_player_five_name"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:lines="1"
                android:textSize="8dp" />

        </LinearLayout>

        <GridLayout
            android:layout_width="10dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:columnCount="1"
            android:rowCount="5">

            <ImageView
                android:id="@+id/game_one_team_two_player_one"
                android:layout_width="10dp"

                android:layout_height="10dp"
                android:layout_column="0"
                android:layout_marginTop="3dp"
                android:layout_row="0" />

            <ImageView
                android:id="@+id/game_one_team_two_player_two"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_column="0"

                android:layout_marginTop="3dp"
                android:layout_row="1" />

            <ImageView
                android:id="@+id/game_one_team_two_player_three"
                android:layout_width="10dp"

                android:layout_height="10dp"
                android:layout_column="0"
                android:layout_marginTop="3dp"
                android:layout_row="2" />

            <ImageView

                android:id="@+id/game_one_team_two_player_four"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_column="0"
                android:layout_marginTop="3dp"
                android:layout_row="3" />

            <ImageView

                android:id="@+id/game_one_team_two_player_five"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_column="0"
                android:layout_marginTop="3dp"
                android:layout_row="4" />


        </GridLayout>

        <LinearLayout
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="4dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/game_one_team_two_player_one_name"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:lines="1"
                android:textSize="8dp" />

            <TextView
                android:id="@+id/game_one_team_two_player_two_name"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:lines="1"
                android:textSize="8dp" />

            <TextView
                android:id="@+id/game_one_team_two_player_three_name"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:lines="1"
                android:textSize="8dp" />

            <TextView
                android:id="@+id/game_one_team_two_player_four_name"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:lines="1"
                android:textSize="8dp" />

            <TextView
                android:id="@+id/game_one_team_two_player_five_name"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:lines="1"
                android:textSize="8dp" />

        </LinearLayout>

    </LinearLayout>

</LinearLayout>